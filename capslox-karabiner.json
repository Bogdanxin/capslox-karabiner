      "description": "Auto generated layer conditions",
      "manipulators": [
        {
          "type": "basic",
          "to": [
            {
              "set_variable": {
                "name": "hyper-mode",
                "value": 1
              }
            }
          ],
          "from": {
            "key_code": "caps_lock"
          },
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "hyper-mode",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
